{
  "name": "typescript-eslint-prettier-config",
  "version": "1.3.4",
  "repository": {
    "type": "git",
    "url": "https://github.com/vsramalwan/typescript-eslint-prettier-config.git"
  },
  "scripts": {
    "dev": "tsc --watch",
    "lint": "eslint --ignore-path .eslintignore --ext .js,.ts .",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "format:check": "prettier --ignore-path .gitignore --check \"**/*.+(js|ts|json)\"",
    "release": "standard-version"
  },
  "keywords": [
    "eslint",
    "prettier",
    "tsconfig",
    "typescript"
  ],
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@types/node": "^17.0.40",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint": "^8.17.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.1",
    "prettier": "^2.6.2",
    "standard-version": "^9.5.0",
    "typescript": "^4.7.3"
  },
  "peerDependencies": {
    "eslint": "^8.17.0",
    "prettier": "^2.6.2",
    "typescript": "^4.7.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "standard-version": {
    "releaseCommitMessageFormat": "chore(release): {{currentTag}}\n[ci skip]"
  }
}
